{
    "name": "Register",
    "id": "register",
    "packaging_format": 1,
    "description": {
        "en": "Une application yunohost pour la crÃ©ation de comptes",
        "fr": "A yunohost app for accounts creation"
    },
    "version": "1.0~ynh1",
    "url": "https://github.com/lelibreauquotidien/register_ynh",
    "upstream": {
        "license": "free",
        "website": "https://github.com/lelibreauquotidien/register_ynh",
        "code": "https://github.com/lelibreauquotidien/register_ynh"
    },
    "license": "free",
    "maintainer": {
        "name": "llaq",
        "email": "contact@lelibreauquotidien.fr"
    },
    "requirements": {
        "yunohost": ">= 4.1.3"
    },
    "multi_instance": true,
    "services": [
        "nginx",
        "uwsgi"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain",
                "example": "example.com"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/example",
                "default": "/example"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            }
        ]
    }
}
